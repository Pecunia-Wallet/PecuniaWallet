{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ngwallet": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/",
                        "deployUrl": "/build/",
                        "baseHref": "/app/",
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": [
                            "zone.js",
                            "src/polyfills.ts"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "assets": [
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/"
                            ]
                        },
                        "scripts": [],
                        "webWorkerTsConfig": "tsconfig.worker.json"
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1.2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "5kb",
                                    "maximumError": "12kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/environment/env.ts",
                                    "with": "src/environment/env.prod.ts"
                                }
                            ]
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environment/env.ts",
                                    "with": "src/environment/env.dev.ts"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "ngwallet:build:production"
                        },
                        "development": {
                            "buildTarget": "ngwallet:build:development"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "proxyConfig": "proxy.conf.json"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "ngwallet:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "webWorkerTsConfig": "tsconfig.worker.json"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ]
                    }
                },
                "ionic-cordova-serve": {
                    "builder": "@ionic/angular-toolkit:cordova-serve",
                    "options": {
                        "cordovaBuildTarget": "ngwallet:ionic-cordova-build",
                        "devServerTarget": "ngwallet:serve"
                    },
                    "configurations": {
                        "production": {
                            "cordovaBuildTarget": "ngwallet:ionic-cordova-build:production",
                            "devServerTarget": "ngwallet:serve:production"
                        }
                    }
                },
                "ionic-cordova-build": {
                    "builder": "@ionic/angular-toolkit:cordova-build",
                    "options": {
                        "browserTarget": "ngwallet:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "ngwallet:build:production"
                        }
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false,
        "schematicCollections": [
            "@angular-eslint/schematics",
            "@ionic/angular-toolkit"
        ]
    },
    "schematics": {
        "@ionic/angular-toolkit:component": {
            "styleext": "scss"
        },
        "@ionic/angular-toolkit:page": {
            "styleext": "scss"
        }
    }
}
